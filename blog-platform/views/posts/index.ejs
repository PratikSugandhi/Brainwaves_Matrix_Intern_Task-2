
<%- include('../partials/header') %>
<h2>All Blog Posts</h2>
<% posts.forEach(post => { %>
  <div>
    <h3><%= post.title %></h3>
    <p><%= post.content %></p>
    <a href="/posts/<%= post.id %>/edit">Edit</a> |
    <a href="/posts/<%= post.id %>/delete">Delete</a>
  </div>
<% }) %>
<%- include('../partials/footer') %>
